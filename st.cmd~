#!/epics/iocs/elauncher/elauncher/bin/linux-x86_64/scriptlaunch

epicsEnvSet("EPICS_CA_ADDR_LIST", "10.0.153.255")
epicsEnvSet("EPICS_CA_AUTO_ADDR_LIST", "NO")
