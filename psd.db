#Pxx_all
record(waveform, "$(P)PSD:X-Wf") {
   field(DESC,"PSD")
   field(NELM,"50001")
   field(FTVL,"DOUBLE")
   field(EGU, "um/sqrt(Hz)")
   field(PREC, "3")
}
#int_Pxx_all
record(waveform, "$(P)PSD:IntX-Wf") {
   field(DESC,"Integral PSD")
   field(NELM,"50001")
   field(FTVL,"DOUBLE")
